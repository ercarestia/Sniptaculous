<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>bool Property</Title>
      <Shortcut>propb</Shortcut>
      <Description>Code snippet for a boolean property.</Description>
    </Header>
    <Snippet>   
      <Declarations>
        <Literal>
          <ID>MyProperty</ID>
          <ToolTip>Replace with the name of the property</ToolTip>
          <Default>MyProperty</Default>
        </Literal>
      </Declarations>       
      <Code Language="csharp">
      <![CDATA[public bool $MyProperty$ { get; set; }$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>